#!/usr/bin/env -S guile -s
# -*- mode: scheme -*-
!#

(write
 '(popup-menu (("[_g] Add tag" add-tag)
               ()
               ("[_a] App" main-menu-call-app)
               ("[_f] Frame" main-menu-call-frame)
               ("[_c] Focused client" main-menu-call-focused-client)
               ("[_t] Current tag" main-menu-call-current-tag)
               ()
               ("[_w] WM" main-menu-call-wm))))
